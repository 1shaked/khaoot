

generator client {
    provider = "prisma-client-py"
    interface = "asyncio"
}

datasource db {
  // could be postgresql or mysql
  provider = "sqlite"
  url      = "file:dev.db"
}

generator db {
  provider             = "prisma-client-py"
  interface            = "asyncio"
  recursive_type_depth = 5
}
// prisma/schema.prisma

model Post {
  id         Int   @id @default(autoincrement())
  title      String
  published  Boolean
  desc       String?
}